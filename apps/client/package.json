{
  "name": "client",
  "version": "0.2.0",
  "packageManager": "pnpm@7.28.0",
  "scripts": {
    "develop": "pnpm run i18n && next dev",
    "build": "NODE_ENV=production pnpm run i18n && next build",
    "serve": "next start",
    "lint": "next lint",
    "i18n:extract": "formatjs extract '{pages,components}/**/*.{js,ts,tsx}' --format simple --id-interpolation-pattern '[sha512:contenthash:base64:6]' --out-file lang/locales/fr.json",
    "i18n:compile": "formatjs compile-folder --ast --format simple lang/locales lang/compiled",
    "i18n": "pnpm run i18n:extract && pnpm run i18n:compile"
  },
  "dependencies": {
    "@next/bundle-analyzer": "^13.1.6",
    "@plaiceholder/next": "2.5.0",
    "@researchgate/react-intersection-observer": "^1.3.5",
    "axios": "1.3.4",
    "framer-motion": "^9.0.4",
    "next": "13.2.1",
    "next-plausible": "^3.7.2",
    "plaiceholder": "2.5.0",
    "react": "18.2.0",
    "react-country-flag": "3.0.2",
    "react-dom": "18.2.0",
    "react-google-recaptcha-v3": "1.10.1",
    "react-hook-form": "7.43.3",
    "react-intersection-observer": "9.4.3",
    "react-intl": "6.2.10",
    "react-markdown": "8.0.5",
    "react-remark": "^2.1.0",
    "react-syntax-highlighter": "^15.5.0",
    "sharp": "0.31.3",
    "styled-components": "5.3.8",
    "vanilla-tilt": "^1.8.0"
  },
  "devDependencies": {
    "@formatjs/cli": "6.0.4",
    "@types/node": "18.14.4",
    "@types/react": "18.0.28",
    "@types/react-dom": "18.0.11",
    "@types/react-syntax-highlighter": "^15.5.6",
    "@types/sharp": "0.31.1",
    "@types/styled-components": "5.1.26",
    "autoprefixer": "10.4.13",
    "babel-plugin-formatjs": "10.4.0",
    "eslint": "8.35.0",
    "eslint-config-next": "13.2.1",
    "postcss": "8.4.21",
    "prettier": "2.8.4",
    "prettier-plugin-tailwindcss": "0.2.4",
    "tailwindcss": "3.2.7",
    "typescript": "4.9.5"
  }
}