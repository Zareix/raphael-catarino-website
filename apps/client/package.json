{
  "name": "client",
  "version": "0.2.0",
  "packageManager": "pnpm@7.18.2",
  "scripts": {
    "develop": "pnpm run i18n && next dev",
    "build": "NODE_ENV=production pnpm run i18n && next build",
    "serve": "next start",
    "lint": "next lint",
    "i18n:extract": "formatjs extract '{pages,components}/**/*.{js,ts,tsx}' --format simple --id-interpolation-pattern '[sha512:contenthash:base64:6]' --out-file lang/locales/fr.json",
    "i18n:compile": "formatjs compile-folder --ast --format simple lang/locales lang/compiled",
    "i18n": "pnpm run i18n:extract && pnpm run i18n:compile"
  },
  "dependencies": {
    "@next/bundle-analyzer": "^13.0.6",
    "@plaiceholder/next": "2.5.0",
    "axios": "0.27.2",
    "framer-motion": "^7.6.19",
    "next": "13.0.6",
    "plaiceholder": "2.5.0",
    "react": "18.2.0",
    "react-country-flag": "3.0.2",
    "react-dom": "18.2.0",
    "react-google-recaptcha-v3": "1.10.1",
    "react-hook-form": "7.40.0",
    "react-intersection-observer": "9.4.1",
    "react-intl": "6.2.5",
    "react-markdown": "8.0.4",
    "sharp": "0.31.2",
    "styled-components": "5.3.6"
  },
  "devDependencies": {
    "@formatjs/cli": "5.1.7",
    "@types/node": "18.11.15",
    "@types/react": "18.0.26",
    "@types/react-dom": "18.0.9",
    "@types/sharp": "0.31.0",
    "@types/styled-components": "5.1.26",
    "autoprefixer": "10.4.13",
    "babel-plugin-formatjs": "10.3.35",
    "eslint": "8.29.0",
    "eslint-config-next": "13.0.6",
    "postcss": "8.4.20",
    "prettier": "2.8.1",
    "prettier-plugin-tailwindcss": "0.2.1",
    "tailwindcss": "3.2.4",
    "typescript": "4.9.4"
  }
}