{
  "name": "client",
  "version": "0.2.0",
  "packageManager": "pnpm@7.14.2",
  "scripts": {
    "develop": "pnpm run i18n && next dev",
    "build": "NODE_ENV=production pnpm run i18n && next build",
    "serve": "next start",
    "lint": "next lint",
    "i18n:extract": "formatjs extract '{pages,components}/**/*.{js,ts,tsx}' --format simple --id-interpolation-pattern '[sha512:contenthash:base64:6]' --out-file lang/locales/fr.json",
    "i18n:compile": "formatjs compile-folder --ast --format simple lang/locales lang/compiled",
    "i18n": "pnpm run i18n:extract && pnpm run i18n:compile"
  },
  "dependencies": {
    "@plaiceholder/next": "2.5.0",
    "axios": "0.27.2",
    "framer-motion": "^7.6.5",
    "next": "12.3.2",
    "plaiceholder": "2.5.0",
    "react": "18.2.0",
    "react-country-flag": "3.0.2",
    "react-dom": "18.2.0",
    "react-google-recaptcha-v3": "1.10.0",
    "react-hook-form": "7.39.1",
    "react-icons": "4.6.0",
    "react-intersection-observer": "9.4.0",
    "react-intl": "6.2.1",
    "react-markdown": "8.0.3",
    "sharp": "0.31.2",
    "styled-components": "5.3.6"
  },
  "devDependencies": {
    "@formatjs/cli": "5.1.3",
    "@types/node": "18.11.9",
    "@types/react": "18.0.25",
    "@types/react-dom": "18.0.8",
    "@types/sharp": "0.31.0",
    "@types/styled-components": "5.1.26",
    "autoprefixer": "10.4.13",
    "babel-plugin-formatjs": "10.3.31",
    "eslint": "8.27.0",
    "eslint-config-next": "12.3.2",
    "postcss": "8.4.18",
    "prettier": "2.7.1",
    "prettier-plugin-tailwindcss": "0.1.13",
    "tailwindcss": "3.2.2",
    "typescript": "4.8.4"
  }
}