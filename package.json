{
  "name": "raphael-catarino-website",
  "type": "module",
  "version": "1.0.0",
  "scripts": {
    "dev": "bunx --bun astro dev",
    "build": "NODE_ENV=production SKIP_ENV_VALIDATION=1 bunx --bun astro build",
    "lint": "bunx --bun astro check",
    "format": "bunx --bun prettier --write .",
    "check": "bun run lint && bun run format",
    "preview": "bunx wrangler pages dev --compatibility-date=2024-12-04 ./dist",
    "astro": "astro"
  },
  "dependencies": {
    "@astrojs/check": "0.9.4",
    "@astrojs/react": "4.1.1",
    "@astrojs/sitemap": "3.2.1",
    "@astrojs/tailwind": "5.1.3",
    "@fontsource-variable/inter": "^5.1.0",
    "@tailwindcss/typography": "^0.5.15",
    "astro": "5.0.8",
    "open-props": "^1.7.7",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "sass": "^1.82.0",
    "tailwindcss": "^3.4.16",
    "vaul": "^1.1.1",
    "wrangler": "^3.92.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/node": "^22.10.1",
    "@types/react": "^18.3.13",
    "@types/react-dom": "^18.3.1",
    "@types/bun": "^1.1.14",
    "prettier": "^3.4.2",
    "prettier-plugin-astro": "^0.14.1",
    "prettier-plugin-tailwindcss": "^0.6.9",
    "typescript": "^5.7.2"
  },
  "trustedDependencies": [
    "@parcel/watcher",
    "esbuild",
    "sharp",
    "workerd"
  ]
}