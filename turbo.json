{
    "$schema": "https://turborepo.org/schema.json",
    "pipeline": {
        "build": {
            "dependsOn": [
                "^build"
            ],
            "outputs": [
                ".next/**",
                "build/**",
                "$NODE_ENV"
            ]
        },
        "lint": {
            "outputs": []
        },
        "develop": {
            "cache": false
        },
        "serve": {
            "cache": false
        }
    }
}